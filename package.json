{
    "name": "conf-ts",
    "version": "1.0.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "dev-test": "ts-node tests/test.ts",
        "dev": "tsc-watch",
        "test": "jest",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "tslint -p tsconfig.json",
        "prepare": "yarn run build",
        "prepublishOnly": "yarn run lint",
        "preversion": "yarn run lint",
        "version": "yarn run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": "https://github.com/Senpai-10/conf-ts.git",
    "homepage": "https://github.com/senpai-10/conf-ts#readme",
    "bugs": {
        "url": "https://github.com/senpai-10/conf-ts/issues"
    },
    "license": "MIT",
    "keywords": [
        "config",
        "config-parser",
        "conf",
        "ini"
    ],
    "author": {
        "name": "senpai-10",
        "url": "https://github.com/senpai-10/",
        "email": "bmjfdrh@gmail.com"
    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-typescript": "^7.17.12",
        "@types/jest": "^28.1.1",
        "@types/node": "^17.0.41",
        "prettier": "^2.6.2",
        "ts-jest": "^28.0.4",
        "ts-node": "^10.8.1",
        "tsc-watch": "^5.0.3",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.7.3"
    },
    "dependencies": {
        "jest": "^28.1.1"
    }
}
